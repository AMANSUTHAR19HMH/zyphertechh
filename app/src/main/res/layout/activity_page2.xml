                <ImageView
                    android:id="@+id/rpqvls7427x"
                    android:layout_width="43dp"
                    android:layout_height="43dp"
                    android:layout_marginEnd="112dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
