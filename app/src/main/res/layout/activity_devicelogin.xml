<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFFFF"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:fillViewport="true"
        android:id="@+id/rnwq5oisa429"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/sff000000sw1bffffffff"
        android:paddingTop="19dp"
        android:clipChildren="false"
        android:clipToPadding="false">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rnept1v8x9b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="46dp"
                android:layout_marginHorizontal="15dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/rcos59sqtg0j"
                    android:textColor="#364abc"
                    android:textSize="22sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="ZYPHER TECH"
                    android:layout_marginTop="50dp"
                    android:gravity="center"
                    />
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/rhoy0yu2v6z"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="356dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/rhbm3z18a5xp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cr9bffd3d6e3"
                        android:elevation="7dp"
                        android:orientation="vertical"
                        android:paddingHorizontal="8dp"
                        android:paddingVertical="50dp">

                        <LinearLayout
                            android:id="@+id/rwbvethyi9us"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="60dp"
                            android:background="@drawable/cr9bffffffff"
                            android:elevation="7dp"
                            android:orientation="vertical"
                            android:padding="15dp">

                            <TextView
                                android:id="@+id/rthul36dx93"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Bluetooth ID"
                                android:textColor="#b9b9b9"
                                android:textSize="20sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/rtgj6xbetyja"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/cr9bffffffff"
                            android:elevation="7dp"
                            android:orientation="vertical"
                            android:paddingHorizontal="11dp"
                            android:paddingVertical="16dp">

                            <TextView
                                android:id="@+id/r54m23ozxek9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="MAC Address"
                                android:textColor="#b9b9b9"
                                android:textSize="20sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </RelativeLayout>
            <LinearLayout
                android:id="@+id/r2wztk3ayqk4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFffffff"
                android:paddingHorizontal="124dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rta038j963ti"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cr100bff000000"
                    android:layout_marginTop="21dp"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">
                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>