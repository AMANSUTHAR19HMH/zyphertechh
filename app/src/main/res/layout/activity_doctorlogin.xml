<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:fillViewport="true"
        android:id="@+id/rkk09bgv6twd"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#FF364abc"
        android:paddingTop="19dp"
        android:paddingBottom="8dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/rdju3ljwuz8h"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="278dp"
                android:layout_marginHorizontal="27dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="57dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:src="@drawable/imm" />
            </LinearLayout>

            <TextView
                android:id="@+id/rauzemeudx7q"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="134dp"
                android:layout_marginBottom="30dp"
                android:text="Login"
                android:textColor="#ffffff"
                android:textSize="40sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/r39peg564ecg"
                android:textColor="#ffffff"
                android:textSize="20sp"
                android:textStyle="bold"
                android:layout_marginBottom="10dp"
                android:layout_marginStart="26dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:hint="STETHOSCOPE ID"
                android:textColorHint="@color/white"
                />
            <LinearLayout
                android:id="@+id/rubag07m072m"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/cr9bffffffff"
                android:layout_marginBottom="39dp"
                android:layout_marginHorizontal="15dp"
                android:elevation="7dp"
                android:orientation="vertical">
            </LinearLayout>

            <EditText
                android:id="@+id/r841wsb1babg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="26dp"
                android:layout_marginBottom="10dp"
                android:hint="PASSWORD"
                android:textColor="#ffffff"
                android:textColorHint="@color/white"
                android:textSize="20sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/rr0gv2vxpkrl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/cr9bffffffff"
                android:layout_marginBottom="228dp"
                android:layout_marginHorizontal="15dp"
                android:elevation="7dp"
                android:orientation="vertical">
                <Button
                    android:id="@+id/loginbtn"
                    android:layout_width="236dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="15dp"
                    android:text="LOGIN"
                    android:textColor="#2312D5"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:backgroundTint="@color/white"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rfwo3ir7wxdr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/cr100bffffffff"
                android:layout_marginHorizontal="124dp"
                android:orientation="vertical">
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>